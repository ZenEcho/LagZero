{
  "name": "lagzero",
  "productName": "LagZero",
  "private": true,
  "version": "0.1.0",
  "main": "dist-electron/index.js",
  "type": "module",
  "author": "LagZero Team",
  "description": "是一款专为极致游戏体验打造的、高性能游戏加速器桌面客户端",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZenEcho/LagZero.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "rebuild:native": "electron-builder install-app-deps",
    "rebuild:sqlite": "pnpm rebuild better-sqlite3",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:win:x64:nsis": "electron-builder --win nsis --x64",
    "dist:win:arm64:nsis": "electron-builder --win nsis --arm64",
    "dist:win:x64:portable": "electron-builder --win portable --x64",
    "dist:win:arm64:portable": "electron-builder --win portable --arm64",
    "dist:win:all": "electron-builder --win nsis portable --x64 --arm64",
    "test": "vitest run --coverage"
  },
  "build": {
    "appId": "com.lagzero.client",
    "productName": "LagZero",
    "compression": "maximum",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist",
      "dist-electron",
      "!**/*.map",
      "!**/node_modules/better-sqlite3/deps/**",
      "!**/node_modules/better-sqlite3/src/**",
      "!**/node_modules/better-sqlite3/build/**/obj/**",
      "!**/node_modules/better-sqlite3/build/**/obj.target/**"
    ],
    "asar": true,
    "electronLanguages": [
      "en-US",
      "zh-CN"
    ],
    "win": {
      "icon": "public/logo.ico",
      "signAndEditExecutable": false,
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "installerIcon": "public/logo.ico",
      "uninstallerIcon": "public/logo.ico",
      "artifactName": "${productName}-Setup-${version}-${os}-${arch}.${ext}"
    },
    "portable": {
      "artifactName": "${productName}-Portable-${version}-${os}-${arch}.${ext}"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "pacman"
      ],
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "category": "Utility"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "ZenEcho",
        "repo": "LagZero"
      }
    ]
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^12.6.2",
    "electron-updater": "^6.7.3",
    "fs-extra": "^11.3.3",
    "kysely": "^0.28.11",
    "tar": "^7.5.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@vueuse/core": "^14.2.0",
    "@iconify-json/carbon": "^1.2.18",
    "@iconify-json/material-symbols": "^1.2.54",
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@types/tar": "^6.1.13",
    "@types/uuid": "^11.0.0",
    "@unocss/reset": "^66.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/tsconfig": "^0.8.1",
    "echarts": "^6.0.0",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "happy-dom": "^20.5.3",
    "js-base64": "^3.7.8",
    "naive-ui": "^2.43.2",
    "pinia": "^3.0.4",
    "sass": "^1.97.3",
    "sortablejs": "^1.15.6",
    "typescript": "~5.9.3",
    "unocss": "^66.6.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vfonts": "^0.0.3",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^4.0.18",
    "vue": "^3.5.24",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.2",
    "vuedraggable": "^4.1.0",
    "vue-tsc": "^3.1.4"
  }
}
